<Window x:Class="HRSH_GameBox.windows.GameConfigWind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HRSH_GameBox.windows"
        mc:Ignorable="d"
        Title="HRSH GameBox Configure Game" Height="293" Width="505" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <CheckBox x:Name="chkRunAsAdmin" Content="Run as administrator" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnSave" Content="Save" Margin="0,0,10,10" BorderBrush="{x:Null}" Background="DodgerBlue" Foreground="White" FontSize="14" Height="30" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="111" Click="btnSave_Click"/>
    </Grid>
</Window>
